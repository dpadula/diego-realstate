{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": [
      "nativewind"
    ],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "/src/components/*"
      ],
      "@constants/*": [
        "constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
